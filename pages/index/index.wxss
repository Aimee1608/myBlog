/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.container{
  width:100%;
  height:100%;
  position: absolute;
  left:0;
  top:0;
  z-index:999;
  background:#fff;
}
.page01{
  width:100%;
  height:100%;
  position: absolute;
  left:0;
  top:0;
  z-index:1;
}
.menu-list{
  width:100%;
  height:30rpx;
  padding:10rpx;
  box-sizing: border-box;
}
.menu-list:after{
  content: '';
  display: block;
  clear:both;
}
.list{
  float:left;
  font-size: 28rpx;
  font-family: Arial, Helvetica, sans-serif;
  padding:0 10rpx;
}

.bg-img{
  width:100%;
  height:393.75rpx;
}
.bg-img swiper-item image{
  width:100%;
  height:393.75rpx;
}